    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="sayi1, sayi2" type="Integer" array="False" size=""/>
            <output expression="&quot;Birinci Say&#305;:&quot;" newline="True"/>
            <input variable="sayi1"/>
            <output expression="&quot;&#304;kinci Say&#305;:&quot;" newline="True"/>
            <input variable="sayi2"/>
            <if expression="sayi1=sayi2">
                <then>
                    <output expression="&quot;girilen say&#305;lar e&#351;ittir.&quot;" newline="True"/>
                </then>
                <else/>
            </if>
            <if expression="sayi1&gt;sayi2">
