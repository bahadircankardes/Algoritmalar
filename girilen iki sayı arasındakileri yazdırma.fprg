                    </while>
                </then>
                <else>
                    <while expression="sayi1&lt;sayi2-1">
                        <assign variable="sayi1" expression="sayi1+1"/>
                        <output expression="&quot;Say&#305;:&quot;&amp;sayi1" newline="True"/>
                    </while>
                </else>
            </if>
        </body>
